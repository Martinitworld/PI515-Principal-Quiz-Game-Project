print ('Welcome to my principal quiz game!')

playing = input("Do you want to play? ")

if playing.lower() != "yes":
    quit()
    
print("Okay! Let's play :)" )
score = 0

answer = input('What does the Pi words in Pi515 stand for? ')
if answer == "pursuit of innovation":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
answer = input('How many college students in the principal program? ')
if answer == "2":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
answer = input('What is Kiana job position in Pi515 ? ')
if answer == "program coordinator":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')

answer = input('Is Nancy a co-founder for Pi515? ')
if answer == "yes":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')    
    
answer = input('Is Principal a gaming company? ')
if answer == "no":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
answer = input('Is Drake University a public or private college? ')
if answer == "private":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
answer = input('Is Python a high level or low level computer language? ')
if answer == "high level":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
answer = input('What year was Python first created? ')
if answer == "1991":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')

answer = input('What is IDE in tech stand for? ')
if answer == "integrated development environment":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')  
    
answer = input('Is Python coming out first or Java coming out first? ')
if answer == "Python":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
print('You got ' + str(score) + ' questions correct!')
print('You got ' + str((score / 10) * 100) + '%.')
